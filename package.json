{
  "name": "my-awesome-portfolio",
  "version": "0.0.1",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx netlify dev",
    "dev:front": "npx vite dev --host 0.0.0.0 --port 3000",
    "dev:back": "npx netlify functions:serve",
    "build": "npx vite build",
    "build:ci": "npm run serve",
    "build:ci22": "npm run start-server",
    "build:ci2": "npm install && npm run build && npm run test:e2e:ci",
    "serve": "npx vite preview --port 3000",
    "start-server": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:dev": "vitest --environment jsdom",
    "test:e2e": "cypress run --e2e",
    "test:e2e:dev": "cypress open --env CYPRESS_BASE_URL=$DEPLOY_URL",
    "test": "npm run test:e2e:ci",
    "test:e2e:ci": "CYPRESS_BASE_URL=$DEPLOY_URL cypress run --e2e",
    "test:e2e:ci2": "start-server-and-test dev http://localhost:3000 test:e2e",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs --fix"
  },
