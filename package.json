{
  "name": "my-awesome-portfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "netlify dev",
    "dev:front": "vite --host 0.0.0.0",
    "dev:back": "netlify functions:serve",
    "dev:test": "cypress open",
    "build": "vite build",
    "serve": "vite preview --port 3000",
    "start-server": "concurrently \"npm run dev:back\" \"npm run serve\"",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "cypress run --e2e",
    "test:e2e:ci": "start-server-and-test start-server http://localhost:3000 test:e2e",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@headlessui/vue": "^1.6.4",
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "concurrently": "7.5.0",
    "faunadb": "4.7.1",
    "flowbite": "^1.4.7",
    "mailgun-js": "^0.22.0",
    "moment": "2.29.4",
    "netlify-plugin-checklinks": "4.1.1",
    "nprogress": "0.2.0",
    "postcss": "^8.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "stylus": "^0.59.0",
    "tailwindcss": "^3.1.3",
    "vee-validate": "^4.5.11",
    "vite": "^2.9.12",
    "vite-plugin-environment": "^1.1.1",
    "vitest": "^0.15.1",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-meta": "^3.0.0-alpha.2",
    "vue-page-transition": "^0.2.2",
    "vue-router": "^4.0.16",
    "vue3-i18n": "1.1.5"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:8888",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "devDependencies": {
    "@vitest/ui": "0.25.1",
    "c8": "7.12.0",
    "cypress": "11.0.1",
    "eslint-plugin-cypress": "2.12.1",
    "netlify-cli": "12.1.1",
    "start-server-and-test": "^1.14.0",
    "vite-plugin-i18n-resources": "^1.0.3"
  }
}
