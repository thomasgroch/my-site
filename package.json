{
  "name": "my-awesome-portfolio",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:front": "vite --host 0.0.0.0 --port 3000",
    "dev:back": "npx netlify functions:serve",
    "dev:all": "concurrently \"npm run dev:front\" \"npm run dev:back\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "start-server-and-test dev:all \"http-get://localhost:3000\" test:e2e",
    "test:e2e": "cypress run --e2e",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write .",
    "check": "npm run lint && npm run format"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "consola": "^3.1.0",
    "date-fns": "^2.29.3",
    "flowbite": "^1.6.4",
    "nprogress": "^0.2.0",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@heroicons/vue": "^2.2.0",
    "@jitsi/vue-sdk": "^1.0.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.13.13",
    "@types/tailwindcss": "^3.1.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "8.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^14.0.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "stylus": "^0.64.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "engines": {
    "node": ">=18"
  }
}
