{
  "name": "my-awesome-portfolio",
  "version": "0.0.1",
  "main": "src/index.js",
  "scripts": {
    "dev": "npx netlify dev",
    "dev:front": "npx vite dev --host 0.0.0.0 --port 3000",
    "dev:back": "npx netlify functions:serve",
    "build": "npx vite build",
    "build:ci": "npm run serve",
    "build:ci22": "npm run start-server",
    "build:ci2": "npm install && npm run build && npm run test:e2e:ci",
    "serve": "npx vite preview --port 3000",
    "start-server": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:dev": "vitest --environment jsdom",
    "test:e2e": "cypress run --e2e",
    "test:e2e:dev": "cypress open --env CYPRESS_BASE_URL=$DEPLOY_URL",
    "test": "npm run test:e2e:ci",
    "test:e2e:ci": "CYPRESS_BASE_URL=$DEPLOY_URL cypress run --e2e",
    "test:e2e:ci2": "start-server-and-test dev http://localhost:3000 test:e2e",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.13",
    "@heroicons/vue": "^2.0.17",
    "@jitsi/vue-sdk": "^1.0.4",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.9",
    "@types/tailwindcss": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "8.0.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "faunadb": "4.8.0",
    "flowbite": "^1.6.5",
    "immutable-ics": "^0.4.0",
    "mailgun.js": "^8.2.1",
    "moment": "2.29.4",
    "nodemailer": "^7.0.3",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nprogress": "0.2.0",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "stylus": "^0.59.0",
    "tailwindcss": "^3.3.1",
    "vee-validate": "^4.8.6",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-page-transition": "^0.2.2",
    "vue-router": "^4.1.6",
    "vue3-i18n": "1.1.5"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:8888",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "0.30.1",
    "c8": "7.13.0",
    "cypress": "^14.0.1",
    "eslint-plugin-cypress": "2.13.2",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "netlify-cli": "^18.0.3",
    "netlify-plugin-checklinks": "^4.1.1",
    "start-server-and-test": "^2.0.0",
    "vite": "^6.0.7",
    "vite-plugin-i18n-resources": "^1.0.3"
  }
}
