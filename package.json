{
  "name": "my-awesome-portfolio",
  "version": "0.0.1",
  "scripts": {
    "dev": "netlify dev",
    "dev:front": "vite dev --host 0.0.0.0",
    "dev:back": "netlify functions:serve",
    "build": "vite build",
    "build:ci": "npm run serve",
    "build:ci22": "npm run start-server",
    "build:ci2": "npm install && npm run build && npm run test:e2e:ci",
    "serve": "vite preview --port 3000",
    "start-server": "concurrently \"npm run dev:back\" \"npm run dev:front\"",
    "test:unit": "vitest run --environment jsdom",
    "test:unit:dev": "vitest --environment jsdom",
    "test:e2e": "cypress run --e2e",
    "test:e2e:dev": "cypress open --env CYPRESS_BASE_URL=$DEPLOY_URL",
    "test": "npm run test:e2e:ci",
    "test:e2e:ci": "CYPRESS_BASE_URL=$DEPLOY_URL cypress run --e2e",
    "test:e2e:ci2": "start-server-and-test dev http://localhost:3000 test:e2e",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.cjs,.mjs --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@headlessui/vue": "^1.7.9",
    "@heroicons/vue": "^2.0.17",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.4",
    "autoprefixer": "^10.4.7",
    "concurrently": "7.6.0",
    "faunadb": "4.7.1",
    "flowbite": "^1.6.4",
    "mailgun.js": "^8.0.6",
    "moment": "2.29.4",
    "netlify-plugin-checklinks": "4.1.1",
    "nodemailer": "^6.8.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "nprogress": "0.2.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "stylus": "^0.59.0",
    "tailwindcss": "^3.2.4",
    "vee-validate": "^4.5.11",
    "vite": "^2.9.12",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^0.28.4",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.2",
    "vue-page-transition": "^0.2.2",
    "vue-router": "^4.0.16",
    "vue3-i18n": "1.1.5"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:8888",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "devDependencies": {
    "@vitest/ui": "0.29.8",
    "c8": "7.12.0",
    "cypress": "11.0.1",
    "eslint-plugin-cypress": "2.12.1",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "netlify-cli": "^12.10.0",
    "start-server-and-test": "^1.14.0",
    "vite-plugin-i18n-resources": "^1.0.3"
  }
}
